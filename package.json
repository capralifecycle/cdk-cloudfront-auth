{
  "name": "@liflig/cdk-cloudfront-auth",
  "version": "0.0.0-development",
  "description": "CDK Constructs for adding authentication for a CloudFront Distribution",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/capralifecycle/cdk-cloudfront-auth"
  },
  "scripts": {
    "build": "rm -rf dist && NODE_OPTIONS=--import=tsx webpack && tsc",
    "watch": "tsc -w",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "npm run build && husky",
    "semantic-release": "semantic-release",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "cdk",
    "cloudfront",
    "authentication"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "dist/**/*",
    "lib/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@aws-cdk/assert": "2.68.0",
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.17.0",
    "@types/aws-lambda": "8.10.147",
    "@types/jest": "29.5.14",
    "@types/jsonwebtoken": "9.0.7",
    "@types/node": "22.10.5",
    "@typescript-eslint/eslint-plugin": "8.19.1",
    "@typescript-eslint/parser": "8.19.1",
    "aws-cdk-lib": "2.174.1",
    "axios": "1.7.9",
    "constructs": "10.4.2",
    "cookie": "1.0.2",
    "eslint": "9.17.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "html-loader": "5.1.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-cdk-snapshot": "2.2.5",
    "jsonwebtoken": "9.0.2",
    "jwks-rsa": "3.1.0",
    "prettier": "3.4.2",
    "semantic-release": "24.2.1",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.1",
    "tsx": "4.19.2",
    "typescript": "5.7.3",
    "webpack": "5.97.1",
    "webpack-cli": "6.0.1"
  },
  "dependencies": {
    "@henrist/cdk-cross-region-params": "^2.0.0",
    "@liflig/cdk-lambda-config": "1.5.7"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.0.0"
  }
}
