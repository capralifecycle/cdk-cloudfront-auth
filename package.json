{
  "name": "@liflig/cdk-cloudfront-auth",
  "version": "0.0.0-development",
  "description": "CDK Constructs for adding authentication for a CloudFront Distribution",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/capralifecycle/cdk-cloudfront-auth"
  },
  "scripts": {
    "build": "rm -rf dist && NODE_OPTIONS=--import=tsx webpack && tsc",
    "watch": "tsc -w",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "lint": "biome check",
    "lint:fix": "biome check --fix",
    "prepare": "npm run build && husky",
    "semantic-release": "semantic-release",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "cdk",
    "cloudfront",
    "authentication"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "dist/**/*",
    "lib/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@aws-cdk/assert": "2.68.0",
    "@biomejs/biome": "2.1.3",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@types/aws-lambda": "8.10.152",
    "@types/jest": "30.0.0",
    "@types/jsonwebtoken": "9.0.10",
    "@types/node": "24.2.0",
    "aws-cdk-lib": "2.208.0",
    "axios": "1.11.0",
    "constructs": "10.4.2",
    "cookie": "1.0.2",
    "html-loader": "5.1.0",
    "husky": "9.1.7",
    "jest": "30.0.5",
    "jest-cdk-snapshot": "2.3.6",
    "jsonwebtoken": "9.0.2",
    "jwks-rsa": "3.2.0",
    "semantic-release": "24.2.7",
    "ts-jest": "29.4.1",
    "ts-loader": "9.5.2",
    "tsx": "4.20.3",
    "typescript": "5.9.2",
    "webpack": "5.101.0",
    "webpack-cli": "6.0.1"
  },
  "dependencies": {
    "@henrist/cdk-cross-region-params": "^2.0.0",
    "@liflig/cdk-lambda-config": "1.7.7"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.0.0"
  }
}
