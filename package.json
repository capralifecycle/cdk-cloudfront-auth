{
  "name": "@liflig/cdk-cloudfront-auth",
  "version": "0.0.0-development",
  "description": "CDK Constructs for adding authentication for a CloudFront Distribution",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/capralifecycle/cdk-cloudfront-auth"
  },
  "scripts": {
    "build": "rm -rf dist && NODE_OPTIONS=--import=tsx webpack && tsc",
    "watch": "tsc -w",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "lint": "biome check",
    "lint:fix": "biome check --fix",
    "fmt": "biome format --write",
    "prepare": "npm run build && husky",
    "semantic-release": "semantic-release",
    "clean": "rm -rf dist",
    "upgrade-deps": "ncu --upgrade --install=always --format group"
  },
  "keywords": [
    "cdk",
    "cloudfront",
    "authentication"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "dist/**/*",
    "lib/**/*"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "devDependencies": {
    "@aws-cdk/assert": "2.68.0",
    "@biomejs/biome": "2.3.13",
    "@commitlint/cli": "20.3.1",
    "@commitlint/config-conventional": "20.3.1",
    "@types/aws-lambda": "8.10.160",
    "@types/jest": "30.0.0",
    "@types/jsonwebtoken": "9.0.10",
    "@types/node": "24.10.9",
    "aws-cdk-lib": "2.235.1",
    "axios": "1.13.4",
    "constructs": "10.4.5",
    "cookie": "1.1.1",
    "html-loader": "5.1.0",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "jest-cdk-snapshot": "2.3.6",
    "jsonwebtoken": "9.0.3",
    "jwks-rsa": "3.2.2",
    "npm-check-updates": "19.3.2",
    "semantic-release": "25.0.2",
    "ts-jest": "29.4.6",
    "ts-loader": "9.5.4",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "webpack": "5.104.1",
    "webpack-cli": "6.0.1"
  },
  "dependencies": {
    "@henrist/cdk-cross-region-params": "^2.0.1",
    "@liflig/cdk-lambda-config": "1.7.28"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.0.0"
  }
}
